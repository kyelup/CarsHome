<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>CarHome</display-name>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-class>com.stee.softserv.carhome.servlet.UploadServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UploadServlet</servlet-name>
		<url-pattern>/servlet/UploadServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>UpBrandServlet</servlet-name>
		<servlet-class>com.stee.softserv.carhome.servlet.UpBrandServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UpBrandServlet</servlet-name>
		<url-pattern>/servlet/UpBrandServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>AddModelForBrandServlet</servlet-name>
		<servlet-class>com.stee.softserv.carhome.servlet.AddModelForBrandServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddModelForBrandServlet</servlet-name>
		<url-pattern>/servlet/AddModelForBrandServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>AddModelBrandActionServlet</servlet-name>
		<servlet-class>com.stee.softserv.carhome.servlet.AddModelBrandActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddModelBrandActionServlet</servlet-name>
		<url-pattern>/servlet/AddModelBrandActionServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>AddCarActionServlet</servlet-name>
		<servlet-class>com.stee.softserv.carhome.servlet.AddCarActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddCarActionServlet</servlet-name>
		<url-pattern>/servlet/AddCarActionServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>AddCarWithNoPicServlet</servlet-name>
		<servlet-class>com.stee.softserv.carhome.servlet.AddCarWithNoPicServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddCarWithNoPicServlet</servlet-name>
		<url-pattern>/servlet/AddCarWithNoPicServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>